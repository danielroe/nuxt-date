{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "ignore": [
    "**/__snapshots__/*"
  ],
  "workspaces": {
    ".": {
      "entry": [
        "build.config.ts",
        "src/module.ts",
        "src/runtime/**/*.{js,ts,mjs,vue}"
      ]
    },
    "playground": {
      "ignoreDependencies": ["nuxt-time"],
      "entry": [
        "workers/*.ts"
      ]
    }
  }
}
